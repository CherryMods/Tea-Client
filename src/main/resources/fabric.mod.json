{
  "schemaVersion": 1,
  "id": "wurst",
  "version": "${version}",

  "name": "Tea Client",
  "description": "K-Client v3, based on Wurst",
  "authors": [
    "Alexander01998",
	"CherrySerenade"
  ],
  "contact": {
    "homepage": "https://github.com/CherryMods/Tea-Client",
    "issues": "https://github.com/CherryMods/Tea-Client/issues",
    "sources": "https://github.com/CherryMods/Tea-Client"
  },

  "license": "GPL-3.0-or-later",
  "icon": "assets/wurst/icon.png",

  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wurstclient.WurstInitializer"
    ]
  },
  "mixins": [
    "wurst.mixins.json"
  ],
  "accessWidener" : "wurst.accesswidener",

  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric-api": ">=0.91.1",
    "minecraft": "~1.20.3-beta.3",
    "java": ">=17"
  },
  "suggests": {
    "mo_glass": "*"
  },
  "breaks": {
    "wi_zoom": "*",
    "wi-zoom": "*",
    "vulkanmod": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Wiki": "https://wurst.wiki/?utm_source=Wurst+Client&utm_medium=ModMenu&utm_content=Wiki",
        "Forum": "https://wurstforum.net/?utm_source=Wurst+Client&utm_medium=ModMenu&utm_content=Forum",
        "Donate": "https://www.wurstclient.net/donate/?utm_source=Wurst+Client&utm_medium=ModMenu&utm_content=Donate"
      }
    }
  }
}
